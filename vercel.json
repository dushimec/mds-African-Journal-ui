{
  "rewrites": [
    {
      "source": "/VOLUME/:volume/ISSUE/:issue/:slug",
      "destination": "https://api.jaedp.org/api/v1/VOLUME/:volume/ISSUE/:issue/:slug"
    },
    {
      "source": "/VOLUME/:volume/ISSUE/:issue/:slug*.pdf",
      "destination": "https://api.jaedp.org/api/v1/VOLUME/:volume/ISSUE/:issue/:slug.pdf"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

